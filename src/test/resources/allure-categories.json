  {
    "name": "Cart Issues",
    "matchedStatuses": ["failed", "broken"],
    "traceRegex": ".*CartPage.*"
  },
  {
    "name": "Checkout Issues",
    "matchedStatuses": ["failed", "broken"],
    "traceRegex": ".*CheckoutPage.*"
  },
  {
    "name": "Element Not Found",
